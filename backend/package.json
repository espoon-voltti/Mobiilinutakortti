{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "compodoc": "npx compodoc -s",
    "compodoc:build": "npx compodoc -p ./tsconfig.json -n 'API'",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint:fix": "tslint --fix -p tsconfig.json -c tslint.json",
    "test": "rm -f ./test/testdb.sql || true && set NODE_ENV=test&& jest --runInBand && rm -f ./test/testdb.sql",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "set NODE_ENV=test&& jest --config ./test/jest-e2e.json --runInBand && rm -f ./test/testdb.sql"
  },
  "dependencies": {
    "@nestjs/axios": "4.0.1",
    "@nestjs/common": "10.4.22",
    "@nestjs/config": "3.3.0",
    "@nestjs/core": "10.4.22",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.4.22",
    "@nestjs/platform-socket.io": "10.4.22",
    "@nestjs/schedule": "6.1.0",
    "@nestjs/swagger": "7.4.2",
    "@nestjs/typeorm": "11.0.0",
    "@node-saml/node-saml": "5.1.0",
    "@node-saml/passport-saml": "5.1.0",
    "@types/express-serve-static-core": "5.1.0",
    "@types/qs": "6.14.0",
    "@types/saml2-js": "3.0.4",
    "bcrypt": "6.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.3",
    "cookie-parser": "1.4.7",
    "date-fns": "4.1.0",
    "express-session": "1.19.0",
    "lodash": "4.17.23",
    "moment": "2.30.1",
    "nestjs-pino": "4.5.0",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "pg": "8.18.0",
    "pixl-xml": "1.0.13",
    "redis": "5.10.0",
    "reflect-metadata": "0.2.2",
    "rimraf": "6.0.1",
    "rxjs": "7.8.2",
    "saml2-js": "4.0.2",
    "swagger-ui-express": "5.0.1",
    "typeorm": "0.3.28",
    "url": "0.11.4",
    "zod": "4.2.1",
    "xmlbuilder2": "4.0.3"
  },
  "devDependencies": {
    "@babel/parser": "7.29.0",
    "@babel/types": "7.29.0",
    "@compodoc/compodoc": "1.2.1",
    "@nestjs/cli": "11.0.16",
    "@nestjs/schematics": "10.2.3",
    "@nestjs/testing": "10.4.22",
    "@types/bcrypt": "6.0.0",
    "@types/cookie-parser": "1.4.10",
    "@types/cron": "2.4.3",
    "@types/jest": "30.0.0",
    "@types/node": "24.10.11",
    "@types/passport-jwt": "4.0.1",
    "@types/prettier": "3.0.0",
    "@types/supertest": "6.0.2",
    "jest": "30.2.0",
    "prettier": "3.8.0",
    "sqlite3": "5.1.7",
    "supertest": "7.2.2",
    "ts-jest": "29.4.0",
    "ts-loader": "9.5.4",
    "ts-node": "10.8.1",
    "tsconfig-paths": "4.2.0",
    "tslint": "6.1.3",
    "typescript": "5.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
